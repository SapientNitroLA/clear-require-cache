{
  "name": "clear-require-cache",
  "version": "0.1.6",
  "description": "Watch files and delete them from `require.cache` when changed.",
  "main": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bump": "sed -e 's/\"$npm_config_tag_version_prefix$npm_package_version\"/\"{version}\"/g' -i README.md && npm version",
    "version": "sed -e 's/\"{version}\"/\"$npm_config_tag_version_prefix$npm_package_version\"/g' -i README.md && git add",
    "postversion": "echo 'push && push --tags'"
  },
  "bin": {
    "clearcache": "./bin/index.js"
  },
  "keywords": [
    "clear",
    "cache",
    "decache",
    "del",
    "delcache",
    "delete",
    "derequire",
    "module",
    "refesh",
    "require",
    "remove",
    "uncache",
    "uncached",
    "unrequire"
  ],
  "author": "Ryan Fitzer",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^1.5.2",
    "commander": "^2.9.0"
  }
}
